    <div id="rightpanel">
        <div id="map" class="map"></div>
        <div id="popup" class="ol-popup">
            <a href="#" id="popup-closer" class="ol-popup-closer"></a>
            <div id="popup-content">
                <div class="popup_chialo"></div>
                <div class="popup_sudungdat"></div>
                <div class="popup_ranhgioi"></div>
            </div>
        </div>

        <div class="mybutton" onclick="attributeQuery()">
            <div class="BaseMap">
                <button class="switcher"></button>
            </div>
            <div class="Tool">

            </div>
        </div>
        @*<div id="" class="mousePosition"></div>*@
    </div>

    <div class="modal" tabindex="-1" role="dialog" id="modalBasemap">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title title-add">Bản đồ nền</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="panel panel-primary">
                    <div class="panel-body">
                        <div class="BaseItem active" v="GoogleMap">
                            <img src="~/assets/images/Bando/imgggmap.png" />
                            <p>Google</p>
                        </div>
                        <div class="BaseItem" v="Satellite">
                            <img src="~/assets/images/Bando/imgvetinh.png" />
                            <p>Vệ tinh</p>
                        </div>
                        <div class="BaseItem" v="OSMStandard">
                            <img src="~/assets/images/Bando/imgosm.jpg" />
                            <p>OSM</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" tabindex="-1" role="dialog" id="modalInfokcn">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title title-add">Thông tin Khu công nghiệp</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="panel panel-primary">
                    <div class="panel-body">
                        <div class="tenKCN">
                        </div>
                        <div class="ndchitiet">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="" id="myDataView" style="display: none;">
        <div class="DataView-content">
            <div class="DataView-header">
                <div class="header-bar">
                    <h5 class="myDataView_modal-title title-add"><b>KCN </b></h5>
                    <div class="myDataView_search">
                        <input type="text" class="form-control" id="myDataView_txtSearchkcn" placeholder="Tìm kiếm khu công nghiệp ...">
                        <button id="myDataView_btnSearchkcn" class="btn btn-primary btn-sm">
                            <i class="fa fa-search"></i>
                        </button>
                    </div>
                    <div id="myDataView_page_navigation"></div>
                    <div class="myDataView_total"><span> Tổng số: 337 </span></div>
                    <div class="myDataView_excels"><i class="far fa-file-excel"></i></div>
                </div>

                <button type="button" class="close close-myDataView" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="DataView-body">
                <div class="DataView-body-content">
                    <table class="table_dataView">
                        <thead>
                            <tr>
                                <th style="width:40px;">STT</th>
                                <th>Khu công nghiệp</th>
                                <th>Ký hiệu lô đất</th>
                                <th>Diện tích (ha)</th>
                                <th>Hiện trạng</th>
                                <th>Doanh nghiệp thuê</th>
                                <th>Quốc tịch</th>
                                <th>Vị trí</th>
                            </tr>
                        </thead>
                        <tbody class="DataView_tbody">
                            <tr>
                                <td>1</td>
                                <td>2</td>
                                <td>3</td>
                                <td>4</td>
                                <td>5</td>
                                <td>6</td>
                                <td>7</td>
                                <td><i class="fas fa-map-marker-alt"></i></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>