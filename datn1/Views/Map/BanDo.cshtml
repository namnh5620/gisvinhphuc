
@{
    ViewBag.Title = "Bản Đồ";
    Layout = "~/Views/Shared/_Layout_Map.cshtml";
}
@section Styles {
    <link href="~/assets/sass/portal/pages/map_bando.css" rel="stylesheet" />
    <link href="~/assets/lib/6.14.1-dist/ol.css" rel="stylesheet" />
}
@section Scripts {
    <script type="text/javascript" src="javascripts/jquery/jquery-2.2.4.min.js"></script>
    <script type="text/javascript" src="javascripts/receiver.js"></script>
    <script src="~/assets/lib/6.14.1-dist/ol.js"></script>
    <script src="~/assets/js/portal/map_bando.js"></script>
}

<div id="bando">
    <div id="leftpanel">
        <div class="title">
            <p><i class="far fa-layer-group"></i> LỚP BẢN ĐỒ</p>
        </div>

        <div class="layers">
            <div class="filter_kcn">
                <div class="search">
                    <input type="text" class="form-control" id="txtSearchkcn" placeholder="Tìm kiếm khu công nghiệp ...">
                    <button id="btnSearchkcn" class="btn btn-primary btn-sm">
                        <i class="fa fa-search"></i>
                    </button>
                </div>
            </div>

            <div class="layer">
                <div class="layer_header">
                    <p data-toggle="collapse" href="#layer_kcns" aria-expanded="false">
                        <i class="fas fa-caret-right">  </i> <i class="far fa-square">  </i> Khu công nghiệp
                    </p>
                </div>

                <div class="collapse layer_body show" id="layer_kcns">
                    <div class="gr_items">
                        <div class="layer_items" id="ranhgioi">
                            <i class="far fa-check-square"></i> <span>Thông tin KCN</span>
                        </div>
                        <i class="fal fa-table thongke"></i>
                    </div>

                    <div class="gr_items">
                        <div class="layer_items" id="chialo">
                            <i class="far fa-check-square"></i> <span>Chia lô sử dụng đất</span>
                        </div>
                        <i class="fal fa-table thongke"></i>
                    </div>

                    <div class="gr_items">
                        <div class="layer_items" id="sudungdat">
                            <i class="far fa-check-square"></i> <span>Tổng mặt bằng sử dụng đất</span>
                        </div>
                        <i class="fal fa-table thongke"></i>
                    </div>

                </div>
            </div>

            <div class="layer">
                <div class="layer_header">
                    <p data-toggle="collapse" href="#layer_nenhc" aria-expanded="false">
                        <i class="fas fa-caret-right">  </i> <i class="far fa-square">  </i> Nền hành chính
                    </p>
                </div>

                <div class="collapse layer_body" id="layer_nenhc">
                    <div class="layer_items" id="commune">
                        <i class="far fa-square"></i> Địa phận xã
                    </div>
                    <div class="layer_items" id="district">
                        <i class="far fa-square"></i> Địa phận huyện
                    </div>
                    <div class="layer_items" id="province">
                        <i class="far fa-square"></i> Địa phận tỉnh
                    </div>
                </div>
            </div>

            <div class="layer_kcn">
            </div>

