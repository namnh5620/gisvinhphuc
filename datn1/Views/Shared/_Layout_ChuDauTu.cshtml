<body>
    <!-- ======= Header ======= -->
    <header id="header">
        <div class="header-top">
            <div class="bg-overlay"></div>
            <div class="container">
                <div class="header-top-wrapper d-flex">
                    <div class="header-left d-flex">
                        <div class="text">
                            <h2 style="font-weight:bold">Phần Mềm Quản Lý Khu Công Nghiệp</h2>
                        </div>
                    </div>
                    <nav id="menu_pc" class="nav-menu d-lg-block d-md-block d-none">
                        <ul>
                            <li class="active" ref="chu-dau-tu-kcn"><a href="~/chu-dau-tu-kcn"><i class="fas fa-file-invoice"></i> Khu Công Nghiệp</a></li>
                            <li ref="chu-dau-tu-user" class="avt_taikhoan"><a href=""><i class="fas fa-user"></i> </a></li>
                            <li ref="trang-chu"><a href="~/"><i class="fas fa-sign-out"></i> Đăng Xuất</a></li>
                        </ul>
                    </nav>

                    <input type="checkbox" id="overlay-input" />
                    <label for="overlay-input" id="overlay-button"><span></span></label>
                    <div id="overlay" class="menu_mobile">
                        <ul>
                            <li class="active" ref="chu-dau-tu-kcn"><a href="~/chu-dau-tu-kcn"><i class="fas fa-file-invoice"></i> Khu Công Nghiệp</a></li>
                            <li ref="chu-dau-tu-user" class="avt_taikhoan"><a href=""><i class="fas fa-user"></i> </a></li>
                            <li ref="trang-chu"><a href="~/"> <i class="fas fa-sign-out"></i> Đăng Xuất</a></li>
                        </ul>
                    </div>

                </div>
            </div>
        </div>
    </header>
    @RenderBody()

