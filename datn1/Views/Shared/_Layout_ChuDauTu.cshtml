        <script src="~/assets/lib/bootstrap4/js/bootstrap.min.js"></script>*@
    <script src="~/assets/lib/bootstrap4/js/bootstrap.bundle.min.js"></script>
    @*<script src="~/DevJS/GQL.js"></script>*@
    <script src="~/assets/js/portal/layout.js"></script>
    <script src="~/assets/js/portal/app.js"></script>
